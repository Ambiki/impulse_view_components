<% if scenarios.many? %>
  <% scenarios.each do |scenario| %>
    <div class="w-100 mb-4">
      <h6 class="text-muted font-weight-normal" style="font-size: 14px;">
        <%= scenario.label %>
      </h6>
      <%= scenario.output %>
    </div>
  <% end %>
<% else %>
  <%= scenarios.first.output %>
<% end %>
